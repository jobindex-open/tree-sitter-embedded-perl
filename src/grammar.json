{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "embedded_perl",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "directive_line"
              },
              {
                "type": "SYMBOL",
                "name": "content_line"
              },
              {
                "type": "SYMBOL",
                "name": "whitespace_line"
              },
              {
                "type": "SYMBOL",
                "name": "newline"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "final_directive_line"
                },
                {
                  "type": "SYMBOL",
                  "name": "final_content_line"
                },
                {
                  "type": "SYMBOL",
                  "name": "line_indent"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "directive_line": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "line_indent"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "SYMBOL",
            "name": "line_statement"
          },
          {
            "type": "SYMBOL",
            "name": "newline"
          }
        ]
      }
    },
    "final_directive_line": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "line_indent"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "SYMBOL",
            "name": "line_statement"
          }
        ]
      }
    },
    "line_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "line_raw_expression"
        },
        {
          "type": "SYMBOL",
          "name": "line_expression"
        },
        {
          "type": "SYMBOL",
          "name": "line_comment"
        },
        {
          "type": "SYMBOL",
          "name": "line_escaped_percent"
        },
        {
          "type": "SYMBOL",
          "name": "line_code"
        }
      ]
    },
    "line_raw_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%=="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "code",
              "content": {
                "type": "SYMBOL",
                "name": "line_content"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "line_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "code",
              "content": {
                "type": "SYMBOL",
                "name": "line_content"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "line_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%#"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "text",
              "content": {
                "type": "SYMBOL",
                "name": "line_content"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "line_escaped_percent": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%%"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "text",
              "content": {
                "type": "SYMBOL",
                "name": "line_content"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "line_code": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "code",
              "content": {
                "type": "SYMBOL",
                "name": "line_content"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "content_line": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_content_start"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_content_piece"
            }
          },
          {
            "type": "SYMBOL",
            "name": "newline"
          }
        ]
      }
    },
    "final_content_line": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_content_start"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_content_piece"
            }
          }
        ]
      }
    },
    "whitespace_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "line_indent"
        },
        {
          "type": "SYMBOL",
          "name": "newline"
        }
      ]
    },
    "_content_start": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "non_directive_text"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "line_indent"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "tag"
                },
                {
                  "type": "SYMBOL",
                  "name": "escaped_open_tag"
                },
                {
                  "type": "STRING",
                  "value": "<"
                }
              ]
            }
          ]
        }
      ]
    },
    "_content_piece": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "tag"
        },
        {
          "type": "SYMBOL",
          "name": "escaped_open_tag"
        },
        {
          "type": "SYMBOL",
          "name": "non_directive_text"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "STRING",
          "value": "<"
        }
      ]
    },
    "tag": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "raw_expression_tag"
        },
        {
          "type": "SYMBOL",
          "name": "expression_tag"
        },
        {
          "type": "SYMBOL",
          "name": "comment_tag"
        },
        {
          "type": "SYMBOL",
          "name": "code_tag"
        }
      ]
    },
    "raw_expression_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<%=="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "code",
              "content": {
                "type": "SYMBOL",
                "name": "tag_content"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "tag_close"
        }
      ]
    },
    "expression_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<%="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "code",
              "content": {
                "type": "SYMBOL",
                "name": "tag_content"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "tag_close"
        }
      ]
    },
    "comment_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<%#"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "text",
              "content": {
                "type": "SYMBOL",
                "name": "tag_content"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "tag_close"
        }
      ]
    },
    "code_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<%"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "code",
              "content": {
                "type": "SYMBOL",
                "name": "tag_content"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "tag_close"
        }
      ]
    },
    "escaped_open_tag": {
      "type": "STRING",
      "value": "<%%"
    },
    "tag_content": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "TOKEN",
            "content": {
              "type": "PREC",
              "value": 1,
              "content": {
                "type": "PATTERN",
                "value": "[^%]+"
              }
            }
          },
          {
            "type": "TOKEN",
            "content": {
              "type": "PREC",
              "value": 1,
              "content": {
                "type": "PATTERN",
                "value": "%[^>]"
              }
            }
          }
        ]
      }
    },
    "tag_close": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "="
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "%>"
        }
      ]
    },
    "line_indent": {
      "type": "PATTERN",
      "value": "[ \\t]+"
    },
    "line_content": {
      "type": "PATTERN",
      "value": "[^\\r\\n]+"
    },
    "text": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "([^<\\r\\n]|<[^%\\r\\n])+"
        }
      }
    },
    "non_directive_text": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 2,
        "content": {
          "type": "PATTERN",
          "value": "[ \\t]*(?:[^<%\\s\\r\\n]|<[^%\\r\\n])(?:[^<\\r\\n]|<[^%\\r\\n])*"
        }
      }
    },
    "newline": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}