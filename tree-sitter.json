{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "embedded_perl",
      "camelcase": "EmbeddedPerl",
      "title": "Embedded Perl templates",
      "scope": "text.html.ep",
      "file-types": [
        "ep",
        "html.ep"
      ],
      "injection-regex": "^embedded_perl$",
      "class-name": "TreeSitterEmbeddedPerl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Embedded Perl templates as used by Mojolicious",
    "authors": [
      {
        "name": "Michael Budde",
        "email": "mbu@jobindex.dk"
      }
    ],
    "links": {
      "repository": "https://github.com/jobindex-open/tree-sitter-embedded-perl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}